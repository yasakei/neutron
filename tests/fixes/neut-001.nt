// NEUT-001: Not-Equal Operator (!=) Produces Incorrect Results
// Testing that != operator works correctly

say("=== NEUT-001: Testing != Operator ===");

var a = 5;
var b = 10;
var c = 5;

// Test basic inequality
if (a != b) {
    say("✓ 5 != 10 is true");
} else {
    say("✗ FAILED: 5 != 10 should be true");
}

// Test equality (should be false)
if (a != c) {
    say("✗ FAILED: 5 != 5 should be false");
} else {
    say("✓ 5 != 5 is false");
}

// Test with strings
var str1 = "hello";
var str2 = "world";
var str3 = "hello";

if (str1 != str2) {
    say("✓ 'hello' != 'world' is true");
} else {
    say("✗ FAILED: 'hello' != 'world' should be true");
}

if (str1 != str3) {
    say("✗ FAILED: 'hello' != 'hello' should be false");
} else {
    say("✓ 'hello' != 'hello' is false");
}

// Test with booleans
var t = true;
var f = false;

if (t != f) {
    say("✓ true != false is true");
} else {
    say("✗ FAILED: true != false should be true");
}

say("=== NEUT-001: All tests passed ===");
