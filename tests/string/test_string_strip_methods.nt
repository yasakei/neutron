say("=== Test String Strip Methods ===");

// Test strip method
var whitespaceStr = "  hello world  ";
var stripped = whitespaceStr.strip();
if (stripped == "hello world") {
    say("✓ strip() basic whitespace removal passed");
} else {
    say("✗ strip() basic whitespace removal failed: '" + stripped + "'");
}

// Test strip with custom characters
var customStr = "xxxhello worldxxx";
var customStripped = customStr.strip("x");
if (customStripped == "hello world") {
    say("✓ strip() custom characters passed");
} else {
    say("✗ strip() custom characters failed: '" + customStripped + "'");
}

// Test lstrip method
var leftStripped = whitespaceStr.lstrip();
if (leftStripped == "hello world  ") {
    say("✓ lstrip() basic whitespace removal passed");
} else {
    say("✗ lstrip() basic whitespace removal failed: '" + leftStripped + "'");
}

// Test rstrip method
var rightStripped = whitespaceStr.rstrip();
if (rightStripped == "  hello world") {
    say("✓ rstrip() basic whitespace removal passed");
} else {
    say("✗ rstrip() basic whitespace removal failed: '" + rightStripped + "'");
}

// Test strip on empty string
var emptyStr = "";
var emptyStripped = emptyStr.strip();
if (emptyStripped == "") {
    say("✓ strip() empty string passed");
} else {
    say("✗ strip() empty string failed: '" + emptyStripped + "'");
}

// Test strip on all whitespace string
var allWhitespace = "   ";
var allWhitespaceStripped = allWhitespace.strip();
if (allWhitespaceStripped == "") {
    say("✓ strip() all whitespace string passed");
} else {
    say("✗ strip() all whitespace string failed: '" + allWhitespaceStripped + "'");
}

// Test strip with tabs and newlines
var complexWhitespace = "\t\n  hello world  \r\n\t";
var complexStripped = complexWhitespace.strip();
if (complexStripped == "hello world") {
    say("✓ strip() complex whitespace passed");
} else {
    say("✗ strip() complex whitespace failed: '" + complexStripped + "'");
}

// Test lstrip with custom characters
var leftCustom = "aaabbbhello worldaaabbb";
var leftCustomStripped = leftCustom.lstrip("ab");
if (leftCustomStripped == "hello worldaaabbb") {
    say("✓ lstrip() custom characters passed");
} else {
    say("✗ lstrip() custom characters failed: '" + leftCustomStripped + "'");
}

// Test rstrip with custom characters
var rightCustomStripped = leftCustom.rstrip("ab");
if (rightCustomStripped == "aaabbbhello world") {
    say("✓ rstrip() custom characters passed");
} else {
    say("✗ rstrip() custom characters failed: '" + rightCustomStripped + "'");
}

say("=== String Strip Methods Tests Complete ===");