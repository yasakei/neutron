say("=== Test String Method Registry ===");

// Property Test 1: Method Registry Consistency
// This test validates that the registry system works correctly
// and that all basic methods are properly registered and functional

var testStr = "hello world";
var passed = 0;
var total = 0;

// Test 1: length method
total = total + 1;
var len = testStr.length;
if (len == 11) {
    say("✓ Registry: length property works");
    passed = passed + 1;
} else {
    say("✗ Registry: length property failed: " + len);
}

// Test 2: contains method
total = total + 1;
var containsResult = testStr.contains("world");
if (containsResult == true) {
    say("✓ Registry: contains method works");
    passed = passed + 1;
} else {
    say("✗ Registry: contains method failed: " + containsResult);
}

// Test 3: contains method negative case
total = total + 1;
var containsNegative = testStr.contains("xyz");
if (containsNegative == false) {
    say("✓ Registry: contains negative case works");
    passed = passed + 1;
} else {
    say("✗ Registry: contains negative case failed: " + containsNegative);
}

// Test 4: split method
total = total + 1;
var parts = testStr.split(" ");
if (parts.length == 2 and parts[0] == "hello" and parts[1] == "world") {
    say("✓ Registry: split method works");
    passed = passed + 1;
} else {
    say("✗ Registry: split method failed");
}

// Test 5: substring method
total = total + 1;
var substr = testStr.substring(0, 5);
if (substr == "hello") {
    say("✓ Registry: substring method works");
    passed = passed + 1;
} else {
    say("✗ Registry: substring method failed: " + substr);
}

// Property Test Summary
say("=== Registry Property Test Results ===");
say("Passed: " + passed + "/" + total);

if (passed == total) {
    say("✓ All registry property tests passed!");
} else {
    say("✗ Some registry property tests failed");
}

say("=== String Method Registry Test Complete ===");